set term svg size 800,600
set output '/var/folders/hq/0d5018vj44x5wg2ynxt_swl40000gn/T/junit16946763524671327745/simple_java.svg'
# Requires data files "[123].dat" from this directory,
# so change current working directory to this directory before running.
# gnuplot> set term <term-type>
# gnuplot> load 'simple.dem'
#
set title "Simple Plots" font ",20"
set key left box
set samples 50
set style data points

plot [-10:10] sin(x),atan(x),cos(atan(x))

set key right nobox
set samples 100
plot [-pi/2:pi] cos(x),-(sin(x) > sin(x+1) ? sin(x) : sin(x+1))

set key left box
set samples 200
plot [-3:5] asin(x),acos(x)

plot [-30:20] besj0(x)*0.12e1 with impulses, [0:*] (x**besj0(x))-2.5 with points

set samples 400
plot [-10:10] real(sin(x)**besj0(x))

set key bmargin center horizontal
plot [-5*pi:5*pi] [-5:5] real(tan(x)/atan(x)), 1/x

set key left box
set autoscale
set samples 800
plot [-30:20] sin(x*20)*atan(x)

plot [-19:19] '1.dat'with impulses ,'2.dat' ,'3.dat' with lines

reset

