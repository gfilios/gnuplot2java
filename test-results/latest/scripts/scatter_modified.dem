set term svg size 800,600
set output '/var/folders/hq/0d5018vj44x5wg2ynxt_swl40000gn/T/junit14426666402732978830/scatter_java.svg'
#
# Simple demo of scatter data conversion to grid data.
#

set title "Simple demo of scatter data conversion to grid data"
unset hidden3d
set ticslevel 0.5
set view 60,30
set autoscale
set parametric
set style data points
set xlabel "data style point - no dgrid"
set key box
splot "hemisphr.dat"

set dgrid3d 10,10 qnorm 1
set xlabel " data style lines, dgrid3d qnorm 1"
set style data lines
splot "hemisphr.dat"

set dgrid3d qnorm 4
set xlabel " data style lines, dgrid3d qnorm 4 "
set style data lines
splot "hemisphr.dat"

set dgrid3d qnorm 16
set xlabel " data style lines, dgrid3d qnorm 16"
set style data lines
splot "hemisphr.dat"

set contour
set xlabel "data style lines, dgrid3d qnorm 16, contour"
splot "hemisphr.dat"

unset dgrid3d
set style data points
set xlabel "data style points, nodgrid3d"
splot "scatter2.dat"

set key nobox
set dgrid3d qnorm 1
set xlabel "data style lines, dgrid3d qnorm 1"
set style data lines
splot "scatter2.dat"

set dgrid3d qnorm 4
set xlabel "data style lines, dgrid3d qnorm 4"
set style data lines
splot "scatter2.dat"
reset
